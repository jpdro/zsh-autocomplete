#autoload

zstyle -t ":autocomplete:tab:" insert-unambiguous && return 1
[[ $WIDGET == _complete_help ]] && return 1
[[ -v compstate[old_list] ]] || return 1
[[ $_autocomplete__buffer == $BUFFER ]] || return 1
[[ $_autocomplete__word == $words[CURRENT] ]] || return 1

compstate[old_list]='keep'
return 0
